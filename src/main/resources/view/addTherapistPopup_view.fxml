<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.piotrziemianek.controller.AddTherapistController">
   <children>
      <Label text="Stopień naukowy" />
      <TextField fx:id="academicDegreeTF" />
      <Label text="Imię" />
      <TextField fx:id="therapistNameTF" />
      <Label text="Nazwisko" />
      <TextField fx:id="therapistSurnameTF" />
      <Button fx:id="addButton" mnemonicParsing="false" text="Dodaj">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
